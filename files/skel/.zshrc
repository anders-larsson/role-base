# /etc/skel/.zshrc

# Default aliases
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias cd..='cd ..'
alias cp='cp -irv'
alias du='du -h --max-depth=1'
alias l='ls -aF --group-directories-first'
alias la='ls -la'
alias ll='ls -CF -alh --group-directories-first'
alias md='mkdir -p'
alias mv='mv -iv'
alias rm='rm -ir'
